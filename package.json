    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-image-picker": "^16.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "^4.0.0",
    "expo-splash-screen": "~0.29.22",
