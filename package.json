{
  "name": "maverick-marketplace",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "setup": "node setup-dev.js",
    "setup-appwrite": "node setup-appwrite.js",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "cross-env WEB=true EXPO_OS=web expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "network": "EXPO_ROUTER_APP_ROOT=./app EXPO_ROUTER_IMPORT_MODE=sync expo start --lan",
    "tunnel": "EXPO_ROUTER_APP_ROOT=./app EXPO_ROUTER_IMPORT_MODE=sync expo start --tunnel",
    "global": "node global-tunnel.js",
    "m1-global": "node m1-mac-global.js",
    "win-setup": "node setup-dev.js",
    "win-start": "set EXPO_ROUTER_APP_ROOT=./app&&set EXPO_ROUTER_IMPORT_MODE=sync&&expo start",
    "win-network": "set EXPO_ROUTER_APP_ROOT=./app&&set EXPO_ROUTER_IMPORT_MODE=sync&&expo start --lan",
    "win-tunnel": "set EXPO_ROUTER_APP_ROOT=./app&&set EXPO_ROUTER_IMPORT_MODE=sync&&expo start --tunnel",
    "win-global": "node global-tunnel.js",
    "restart-appwrite": "cd appwrite && docker-compose down && docker-compose up -d"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.3.0",
